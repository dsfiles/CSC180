int[] count = new int[4];
count[0] = 7;
count[1] = count[0] * 2;
count[2]++;
count[3] -= 60;

Console.WriteLine("Count");
int index = 0;
while (index < 4)
{
    Console.WriteLine(count[index]);
    index++;
}

Console.WriteLine("Count2");
int[] count2 = count;
for (int i = 0; i < 4; i++)
{
    Console.WriteLine(count2[i]);
}
