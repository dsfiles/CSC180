int[] count = new int[4];
count[0] = 7;
count[1] = count[0] * 2;
count[2]++;
count[3] -= 60;

Console.WriteLine("Count");
int index = 0;
while (index < 4)
{
    Console.WriteLine(count[index]);
    index++;
}

int[] count2 = new int[4]; 
for (int i = 0; i < 4; i++)
{
    count2[i]=count[i];
}

count[0] = 99;
Console.WriteLine("Count2");
for (int i = 0; i < 4; i++)
{
    Console.WriteLine(  count2[i] );
}
